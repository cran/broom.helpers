Package: broom.helpers
Title: Helpers for Model Coefficients Tibbles
Version: 1.10.0
Authors@R: c(
    person("Joseph", "Larmarange", , "joseph@larmarange.net", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0001-7097-700X")),
